                <th>VêlagePrévu</th>
                <td>{{ gestation.vêlagePrévu ? gestation.vêlagePrévu|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateAvertissement</th>
                <td>{{ gestation.dateAvertissement ? gestation.dateAvertissement|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gestation_index') }}">back to list</a>

