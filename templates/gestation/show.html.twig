{% extends 'base.html.twig' %}

{% block title %}Gestation{% endblock %}

{% block body %}
    <h1>Gestation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gestation.id }}</td>
            </tr>
            <tr>
                <th>Espece</th>
                <td>{{ gestation.espece }}</td>
            </tr>
            <tr>
                <th>PreparationVêlage</th>
